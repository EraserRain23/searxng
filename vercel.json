{
  "builds": [
    { 
      "src": "searxng/requirements.txt",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "includeFiles": "searxng/**"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "searxng/static/$1" },
    { "src": "/(.*)", "dest": "searxng/wsgi.py" }
  ],
  "functions": {
    "searxng/wsgi.py": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
